@model Modularbeit183.Models.UserModel

@{
    Layout = null;
}
<div class="text-center">
<form action="Login" method="post">
    <div class="form-group">
        <label for="Benutzername">Benutzername</label>
        <input type="text" class="form-control" id="Benutzername" aria-describedby="Benutzername" placeholder="Benutzername" asp-for="Username">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="Passwort">Passwort</label>
        <input type="password" class="form-control" id="Passwort" placeholder="Passwort" asp-for="Passwort">
    </div>
     
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

</div>